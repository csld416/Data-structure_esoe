{
    "python.pythonPath": "/Users/zjx/anaconda3/bin/python3",
    "code-runner.executorMap": {
        "c": "cd $dir && clang++ $fileName -o $dir/output/$fileNameWithoutExt && ./output/$fileNameWithoutExt",
